const pageTitle = "Etusivu";
---

<BaseLayout pageTitle={pageTitle}>
  <main class="px-5 pb-16 md:px-[5.625rem]">
    <section class="px-0 md:px-20">
      <div class="pb-16 pt-10 text-center">
        <h1
          class="pb-4 text-center text-sm font-medium leading-tight tracking-[1rem] text-u+burg md:text-[2.5rem]"
        >
          URAOHJAIN
        </h1>
        <span
          class="text-center text-sm font-light leading-tight text-u+burg md:text-[2.5rem]"
          >ICT-, ohjelmistokehitys- ja media-alan työnhakijoille</span
        >
      </div>
      <p class="mb-4 text-balance text-center leading-9 md:mb-14 md:text-2xl">
        Tule täydentämään osaamistasi työelämän nopeita muutostarpeita
        vastaavaksi, suunnittelemaan uraasi ja oivaltamaan lisää oman osaamisesi
        markkinoinnista. Pääset keskustelemaan oman arkesi hyvinvoinnista
        työllistymisen tueksi. Osaamiskokonaisuudet rakentuvat Laurean ja
        Metropolian ICT-, ohjelmistokehitys- ja media-alojen opintotarjonnasta.
      </p>
    </section>
    <section>
      <div
        class="border-4 border-u+green px-6 py-12 md:basis-3/5 md:px-12 md:py-12 text-balance text-center"
